{
  "name": "tumbach-pages",
  "version": "1.0.0",
  "scripts": {
    "start": "parcel ./src/**/*.html",
    "build": "parcel build",
    "update-browserslist-db": "update-browserslist-db"
  },
  "targets": {
    "ansi": {
      "source": [
        "./src/ansi/index.html",
        "./src/ansi/2023-01.html",
        "./src/ansi/403.html"
      ],
      "distDir": "./public/ansi",
      "publicUrl": "."
    },
    "amd": {
      "source": [
        "./src/amd/index.en.html",
        "./src/amd/index.ru.html",
        "./src/amd/2023-07.en.html",
        "./src/amd/2023-07.ru.html",
        "./src/amd/2024-01.en.html",
        "./src/amd/2024-01.ru.html"
      ],
      "distDir": "./public/amd",
      "publicUrl": "."
    },
    "announce": {
      "source": [
        "./src/announce/index.html"
      ],
      "distDir": "./public/announce",
      "publicUrl": "."
    }
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "dependencies": {
    "parcel": "2.13.3",
    "tinygradient": "1.1.5"
  },
  "devDependencies": {
    "@parcel/compressor-brotli": "2.13.3",
    "@parcel/compressor-gzip": "2.13.3",
    "@parcel/config-default": "2.13.3",
    "@parcel/core": "2.13.3",
    "svgo": "3.3.2",
    "update-browserslist-db": "1.1.2"
  }
}
